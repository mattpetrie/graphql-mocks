{
  "env": {
    "node": true,
  },
  "extends": [
    "airbnb",
    "prettier",
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "prettier/prettier": ["error", {"singleQuote": true, "semi": false, "trailingComma": "es5"}],
  }
}
